<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="pos_shop_bill_button.bill_button">
        <span class="control-button order-bill-button btn btn-light rounded-0 fw-bolder" t-on-click="() => this.click()">
            <i class="fa fa-print"></i>
            <span> </span>
            <span>Bill</span>
        </span>
    </t>
</templates>


